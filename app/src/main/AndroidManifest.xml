<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.snooze.snooze">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.snooze.snooze.AboutSnooze"></activity>
        <activity android:name="com.snooze.snooze.Settings" />
        <activity android:name="com.snooze.snooze.Maps" />
        <activity android:name="com.snooze.snooze.Menu" />
        <activity android:name="com.snooze.snooze.Register" />
        <activity android:name="com.snooze.snooze.LogIn" />
        <activity android:name="com.snooze.snooze.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>